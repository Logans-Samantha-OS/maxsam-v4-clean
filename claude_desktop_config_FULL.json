    "alex-knowledge": {
      "command": "python",
      "args": ["-m", "alex_knowledge_mcp"],
      "env": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY",
        "OPENAI_API_KEY": "YOUR_OPENAI_API_KEY"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-filesystem", "C:\\Users\\MrTin\\Downloads\\MaxSam-V4"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-memory"]
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "supabase-mcp-server"],
      "env": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY"
      }
    },
    "browserless": {
      "command": "node",
      "args": ["C:\\Users\\MrTin\\Downloads\\MaxSam-V4\\mcp-servers\\browserless-mcp\\dist\\index.js"],
      "env": {
        "BROWSERLESS_API_KEY": "YOUR_BROWSERLESS_API_KEY",
        "BROWSERLESS_URL": "wss://production-sfo.browserless.io"
      }
    },
    "apify": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-apify"],
      "env": {
        "APIFY_API_TOKEN": "YOUR_APIFY_API_TOKEN"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-github"],
      "env": {
        "GITHUB_TOKEN": "YOUR_GITHUB_TOKEN"
      }
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-sequential-thinking"]
    }
  }
}
